{
sum_input <- rowSums(input_db[, 4:9])
}
else if (irrig==F)
{
ifelse(is.na(input_db[, length(input_db)]), 0)
sum_input <- rowSums(input_db[, 4:10])
}
else
{
print('This is awkward.')
}
gnb_organize(input_db, sum_input)
}
