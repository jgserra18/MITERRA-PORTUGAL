set_dir <- function(dir_path)
{
if (missing(dir_path)==TRUE)
{
path <- readline(prompt="Directory: ")
path <- as.character(path)
setwd(path)
}
else
{
dir_path <- dir_path
setwd(dir_path)
}
}
